---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

    <title>Eric GELOEN</title>
    
    <link href="http://egeloen.github.com/atom.xml" rel="self"/>
    <link href="http://egeloen.github.com/"/>
    
    <updated>{{ site.time | date_to_xmlschema }}</updated>
    <id>http://egeloen.github.com/</id>
    <author>
        <name>Eric GELOEN</name>
        <email>geloen.eric@gmail.com</email>
    </author>

    {% for post in site.posts %}
        <entry>
            <title>{{ post.title }}</title>
            <link href="http://egeloen.github.com{{ post.url }}"/>
            <updated>{{ post.date | date_to_xmlschema }}</updated>
            <id>http://egeloen.gith.com{{ post.id }}</id>
            <content type="html">{{ post.content | xml_escape }}</content>
        </entry>
    {% endfor %}

</feed>
